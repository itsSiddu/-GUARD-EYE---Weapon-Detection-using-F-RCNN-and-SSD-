{
  "name": "Test2",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This is my first Google Chrome extension.",
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
